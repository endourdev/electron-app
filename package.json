{
  "name": "electron-app",
  "version": "0.0.3",
  "description": "A simple electron app for testing auto updates.",
  "license": "ISC",
  "author": "Endour",
  "type": "commonjs",
  "main": "main.js",
  "repository": "https://github.com/endourdev/electron-app",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.example.app",
    "productName": "Electron App",
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "distv3"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron-builder": "^26.0.12",
    "electron": "^39.2.7",
    "nodemon": "^3.1.11"
  },
  "dependencies": {
    "electron-updater": "^6.6.2"
  }
}
